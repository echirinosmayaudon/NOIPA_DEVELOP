    }

    .cbp-spmenu-bottom {
        bottom: -110px;
    }
}

@media screen and (max-height: 26.375em) {
    .cbp-spmenu-vertical {
        width: 280px;
    }

    .cbp-spmenu-left,
    .push-body-toleft {
        left: -280px;
    }

    .cbp-spmenu-right {
        right: -280px;
    }

    .push-body-toright {
        left: 280px;
    }
} 

